#!/bin/sh

/usr/local/bin/gmtkJT -iswp1 -of1 GM_fa2014/flists/test_features.flist -nf1 62 -fmt1 htk \
        -strFile GM_fa2014/fa2014.str \
        -inputMasterFile GM_fa2014/fa2014.master \
        -inputTrainableParameters GM_fa2014/fa2014_learned_params.gmp \
        -pCliquePrintRange 0 \
        -cCliquePrintRange 0 \
        -eCliquePrintRange 0 \
        -cliqueOutputFileName GM_fa2014/fa2014.post \
        -cliqueListFileName GM_fa2014/fa2014.post \
        -cliquePrintFormat ascii

