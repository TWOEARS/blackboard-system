<?xml version="1.0" encoding="utf-8"?>
<!-- Configure dependency of the Two!Ears Blackboard System module -->

<!-- Start the Two!Ears Blackboard System with
     startTwoEars('BlackboardSystem.xml'); -->

<requirements>
    <TwoEarsPart branch="initOnConstruction"     sub="src" startup="startBinauralSimulator">binaural-simulator</TwoEarsPart>
    <TwoEarsPart branch="SignalBufferDvlp"       sub="src" startup="startAuditoryFrontEnd">auditory-front-end</TwoEarsPart>
    <TwoEarsPart branch="identTraining_refactor" sub="src" startup="startBlackboardSystem">blackboard-system</TwoEarsPart>
    <TwoEarsPart branch="renaming"               sub="src" startup="startTools" >tools</TwoEarsPart>
</requirements>
